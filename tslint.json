{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "object-literal-sort-keys": false,
    "max-classes-per-file": [false, 999],
    "no-console": false,
    "quotemark": [true, "single", "avoid-escape", "avoid-template"],
    "trailing-comma": [
      false,
      {
        "multiline": {
          "objects": "never",
          "arrays": "never",
          "functions": "never",
          "typeLiterals": "ignore"
        },
        "esSpecCompliant": true
      }
    ]
  },
  "rulesDirectory": []
}
